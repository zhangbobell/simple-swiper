{
  "name": "simple-swiper",
  "version": "1.0.0",
  "description": "a simple swiper",
  "main": "src/swiper.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "dev": "gulp watch",
    "build": "gulp",
    "test": "jest"
  },
