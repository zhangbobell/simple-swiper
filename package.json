  },
  "homepage": "https://github.com/zhangbobell/simple-swiper#readme",
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-concat": "^2.6.1",
    "gulp-copy": "^1.0.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^3.0.0",
    "jest": "^20.0.4",
    "rewire": "^2.5.2",
    "run-sequence": "^2.0.0"
